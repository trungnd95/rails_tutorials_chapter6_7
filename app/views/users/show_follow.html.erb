<%= content_for :title do %>
<title><%= @title %></title>
<% end %>
<%= content_for :page_title do %>
<%= @title %>
<% end %>

<div class="col-md-5">
	<h2><strong><%= @fo_user.name  %></strong></h2>
	<hr />
	<%= render 'shared/stats' %>
</div>
<div class="col-md-7">
	<h4>List</h4>
	<% if @users.any? %>
		<% @users.each do |user| %>
			<li><%= link_to user.name , user_path(user.id) %></li>
		<% end %>
		<%= will_paginate @users %>
	<% end %>
</div>
